{
  "name": "angular2-seed",
  "version": "1.0.0",
  "description": "A simple starter Angular2 project",
  "scripts": {
    "build": "webpack --progress",
    "watch": "npm run build -- --watch",
    "server": "webpack-dev-server --inline --progress --port 3000 --content-base src",
    "start:old": "npm run watch && nodemon ./server/server.js",
    "watch:server": "nodemon \"./server/server.js\" --watch \"./server\"",
    "watch:build": "webpack --watch",
    "start": "concurrently \"npm run watch:build\" \"npm run watch:server\""
  },
  "contributors": [
    "Rob Wormald <robwormald@gmail.com>",
    "PatrickJS <github@gdi2290.com>"
  ],
  "license": "MIT",
